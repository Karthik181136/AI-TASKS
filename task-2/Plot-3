import matplotlib.pyplot as plt
plt.style.use('classic')
import numpy as np
x = ['France','USA','Yugoalavia','Romania','Argentina','Chile','Yugoslavia','USA','Uruguay']
y = [4,3,2,3,1,3,4,3,1]
plt.xlabel("COUNTRY")
plt.ylabel("HOME TEAM GOALS")
plt.title("BAR PLOT")
plt.bar(x,y,color='orange')
plt.legend()
plt.show()
